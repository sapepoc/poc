<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:camel="http://camel.apache.org/schema/spring"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd        http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring.xsd">
    <!-- Define all Persistence Routes below in persistenceRoutes routeContext -->
    <routeContext id="ruleEnginRoutes" xmlns="http://camel.apache.org/schema/spring">
        <route autoStartup="true" id="ruleEnginRoute">
            <from uri="activemq:INSTANCE_ENTITIES"/>
            <process ref="entityProcessor"/>
        </route>
    </routeContext>
</beans>
